{
	"name": "react-parcel-tsx-starter",
	"description": "React Parcel TSX Starter",
	"scripts": {
		"prebuild": "rmdir dist /s /q",
		"build": "parcel build ./src/index.html --no-content-hash",
		"build:prod": "parcel build ./src/index.html --no-content-hash --no-source-maps",
		"start": "parcel ./src/index.html --port 8080 --open"
	},
	"dependencies": {
		"@types/node": "^14.14.31",
		"bootstrap": "^4.6.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"cssnano": "^4.1.10",
		"sass": "^1.32.8",
		"typescript": "^4.2.3"
	}
}
